[project]
	description = my dotfiles
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Vincent Pucci (vpucci)
[access "refs/for/refs/*"]
	read = group user/Vincent Pucci (vpucci)
	push = group user/Vincent Pucci (vpucci)
	label-Code-Review = -2..+2 group user/Vincent Pucci (vpucci)
	label-Verified = -1..+1 group user/Vincent Pucci (vpucci)
	rebase = group user/Vincent Pucci (vpucci)
	removeReviewer = group user/Vincent Pucci (vpucci)
	submit = group user/Vincent Pucci (vpucci)
[access "refs/heads/*"]
	read = group user/Vincent Pucci (vpucci)
	create = group user/Vincent Pucci (vpucci)
	push = +force group user/Vincent Pucci (vpucci)
	pushMerge = group user/Vincent Pucci (vpucci)
[access "refs/tags/*"]
	push = +force group user/Vincent Pucci (vpucci)
	pushTag = group user/Vincent Pucci (vpucci)
	pushSignedTag = group user/Vincent Pucci (vpucci)
